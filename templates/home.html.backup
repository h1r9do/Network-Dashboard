    <script>
        // Add page transition effects
        document.addEventListener('DOMContentLoaded', function() {
            // Animate cards on page load
            const cards = document.querySelectorAll('.card');
            cards.forEach((card, index) => {
                card.style.opacity = '0';
                card.style.transform = 'translateY(20px)';
                
                setTimeout(() => {
                    card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
                    card.style.opacity = '1';
                    card.style.transform = 'translateY(0)';
                }, index * 100);
            });
            
            // Add ripple effect on click
            cards.forEach(card => {
                card.addEventListener('click', function(e) {
                    const ripple = document.createElement('div');
                    ripple.className = 'ripple';
                    this.appendChild(ripple);
                    
                    const rect = this.getBoundingClientRect();
                    const x = e.clientX - rect.left;
                    const y = e.clientY - rect.top;
                    
                    ripple.style.left = x + 'px';
                    ripple.style.top = y + 'px';
                    
                    setTimeout(() => ripple.remove(), 600);
                });
            });
            
            // Add special handler for System Health card
            const healthCard = document.querySelector('.card-health');
            if (healthCard) {
                healthCard.addEventListener('click', function(e) {
                    e.preventDefault();
                    // Navigate to performance monitoring page
                    window.location.href = '/performance';
                });
            }
            
            // Load real-time data for System Health
            loadSystemHealth();
            setInterval(loadSystemHealth, 30000); // Refresh every 30 seconds
        });
        
        // Function to load system health data
        function loadSystemHealth() {
            fetch('/api/health')
                .then(response => response.json())
                .then(data => {
                    // Update database status
                    const dbStatus = document.getElementById('db-status');
                    const dbContainer = dbStatus.parentElement.parentElement;
                    if (data.database === 'healthy') {
                        dbStatus.textContent = 'Healthy';
                        dbStatus.style.color = '#2ecc71';
                        dbContainer.style.borderLeftColor = '#2ecc71';
                    } else {
                        dbStatus.textContent = 'Error';
                        dbStatus.style.color = '#e74c3c';
                        dbContainer.style.borderLeftColor = '#e74c3c';
                    }
                    
                    // Update cache status
                    const cacheStatus = document.getElementById('cache-status');
                    const cacheContainer = cacheStatus.parentElement.parentElement;
                    if (data.cache === 'healthy') {
                        cacheStatus.textContent = 'Active';
                        cacheStatus.style.color = '#2ecc71';
                        cacheContainer.style.borderLeftColor = '#2ecc71';
                    } else {
                        cacheStatus.textContent = 'Inactive';
                        cacheStatus.style.color = '#e74c3c';
                        cacheContainer.style.borderLeftColor = '#e74c3c';
                    }
                    
                    // Update version
                    const versionStatus = document.getElementById('version-status');
                    versionStatus.textContent = data.version || '2.0.0';
                    
                    // Update API status
                    const apiStatus = document.getElementById('api-status');
                    const apiContainer = apiStatus.parentElement.parentElement;
                    if (data.status === 'healthy') {
                        apiStatus.textContent = 'Online';
                        apiStatus.style.color = '#2ecc71';
                        apiContainer.style.borderLeftColor = '#2ecc71';
                    } else {
                        apiStatus.textContent = 'Offline';
                        apiStatus.style.color = '#e74c3c';
                        apiContainer.style.borderLeftColor = '#e74c3c';
                    }
                    
                    // Update overall status message
                    const statusMessage = document.getElementById('system-status-message');
                    if (data.status === 'healthy') {
                        statusMessage.innerHTML = '<div style="font-size: 10px; color: #2e7d32;">✓ All systems operational</div>';
                        statusMessage.style.background = '#e8f5e9';
                    } else {
                        statusMessage.innerHTML = '<div style="font-size: 10px; color: #d32f2f;">⚠ System issues detected</div>';
                        statusMessage.style.background = '#ffebee';
                    }
                })
                .catch(error => {
                    console.error('Error loading health data:', error);
                    document.getElementById('system-status-message').innerHTML = 
                        '<div style="font-size: 10px; color: #d32f2f;">⚠ Unable to fetch system status</div>';
                });
        }
    </script>
    
    <style>
        /* Ripple effect */
        .card {
            position: relative;
            overflow: hidden;
        }
        
        .ripple {
            position: absolute;
            width: 0;
            height: 0;
            border-radius: 50%;
            background: rgba(255, 255, 255, 0.5);
            transform: translate(-50%, -50%);
            animation: ripple 0.6s ease-out;
        }
        
        @keyframes ripple {
            to {
                width: 400px;
                height: 400px;
                opacity: 0;
            }
        }
    </style>
</body>
</html>