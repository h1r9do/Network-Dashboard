                    <th>WAN 2 Speed</th>
                    <th>WAN 2 Cost</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody id="circuitData">
                {% if error %}
                    <tr>
                        <td colspan="8">{{ error }}</td>
                    </tr>
                {% else %}
                    {% for entry in grouped_data %}
                        <tr>
                            <td>
                                {{ entry.network_name }}
                                {% if entry.is_remodeling %}
                                    <span class="remodeling-badge">üî® Remodeling</span>
                                {% endif %}
                            </td>
